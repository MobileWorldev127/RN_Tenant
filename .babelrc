{
	"presets": [
		"module:metro-react-native-babel-preset"
	],
	"plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "extensions": [".js", ".ios.js", ".android.js"],
        "alias": {
          "@src": "./src"
        }
			}
		],
		[
			"module:react-native-dotenv", {
					"moduleName": "@env",
					"path": ".env",
					"blacklist": null,
					"whitelist": null,
					"safe": false,
					"allowUndefined": false
			}
	],
  ]
}
